{
    "name": "Startpage",
    "description": "Simple startpage with bookmarks.",
    "version": "1",
    "host_permissions": ["<all_urls>"],
    "permissions": [
        "activeTab",
        "tabs",
        "bookmarks",
        "storage"
    ],
    "manifest_version": 3,
    "chrome_url_overrides":{
        "newtab":"index.html",
        "homepage":"index.html"
    },
    "icons": {
        "96":"icon.png"
    },
    "options_page":"config/configuration.html"
}